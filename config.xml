<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns		= "http://www.w3.org/ns/widgets"
	xmlns:gap	= "http://gateway.icn.org.au/mobile"
	id		= "au.org.icn.gateway.mobile"
	version 	= "1.0.0">

	<name>ICN Gateway - Mobile</name>

	<description>
		Industry Capability Network Gateway - Mobile
	</description>

	<author href="http://www.icn.org.au"
		email="info@icn.org.au">
		Industry Capability Network Limited
	</author>

	<gap:platforms>
		<gap:platform name="android" minVersion="2.1" />
		<gap:platform name="webos" />
		<gap:platform name="symbian.wrt" />
		<gap:platform name="blackberry" project="widgets"/>
	</gap:platforms>

	<icon src="blurry.png" gap:role="default" />

    <gap:splash src="weirdo.png" />

	<feature name="http://api.phonegap.com/1.0/geolocation"/>
	<feature name="http://api.phonegap.com/1.0/network"/>

	<!-- sample preference specification -->
	<!-- <preference name="autorotate" value="false" readonly="true"/> -->
</widget>
